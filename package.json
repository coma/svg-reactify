{
    "name"           : "svg-reactify",
    "description"    : "transform SVG files into React elements",
    "homepage"       : "https://github.com/coma/svg-reactify",
    "repository"     : "https://github.com/coma/svg-reactify",
    "main"           : "./index.js",
    "version"        : "1.1.0",
    "license"        : "MIT",
    "private"        : false,
    "author"         : "Eduardo García Sanz <eduardo@comakai.com>",
    "contributors"   : [
        "Matthew Brandly <matt@brandly.me>",
        "Anders Stalheim Øfsdahl <https://github.com/andersaloof>"
    ],
    "keywords"       : [
        "browserify",
        "react",
        "reactjs",
        "svg",
        "browserify-transform"
    ],
    "dependencies"   : {
        "through"          : "2.3",
        "react"            : "0.14",
        "react-addons-test-utils": "0.14",
        "babel-core"       : "6.3",
        "babel-preset-es2015": "6.3",
        "babel-preset-react": "6.3",
        "svgo"             : "0.5",
        "extend"           : "3.0",
        "underscore.string": "3.2"
    },
    "devDependencies": {
        "jsdom" : "5.4",
        "mocha" : "2.2",
        "should": "7.0",
        "eslint": "1.1"
    },
    "scripts"        : {
        "lint": "eslint index.js",
        "test": "npm run lint && mocha"
    },
    "engines"        : {
        "iojs": ">= 1.0.0",
        "node": ">= 0.11.0"
    },
    "eslintConfig"   : {
        "env"         : {
            "node": true
        },
        "rules"       : {
            "strict"                     : 0,
            "new-cap"                    : 0,
            "eqeqeq"                     : 2,
            "curly"                      : 2,
            "quotes"                     : [
                1,
                "single"
            ],
            "no-unreachable"             : 2,
            "space-before-blocks"        : 2,
            "space-before-function-paren": 2,
            "no-multi-spaces"            : 0,
            "key-spacing"                : 0,
            "no-mixed-requires"          : 0,
            "no-use-before-define"       : 0,
            "no-underscore-dangle"       : 0
        }
    }
}
